       $(function () {
           setShowHeader();
           $('.service-ul li').click(function () {
               $(this).siblings('li').css('border-left','none');
               $(this).css('border-left','5px solid #FF6A6A');
               var id=$(this).attr('data-id');
               //$('.service-con-title').text($(this).text());
               if(id!=undefined){
                   $('.service-container>div').hide();
                   $('.service-container>div').eq(id).show();
               }
           });
       });
